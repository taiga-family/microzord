{
    "name": "microzord",
    "version": "2.2.0",
    "private": true,
    "keywords": [
        "microfrontends",
        "single-page-application",
        "framework",
        "angular",
        "react",
        "routing",
        "microservices"
    ],
    "homepage": "https://taiga-family.github.io/microzord",
    "repository": "https://github.com/taiga-family/microzord",
    "license": "Apache-2.0",
    "contributors": [
        "Igor Katsuba <katsuba.igor@gmail.ru>",
        "Roman Sedov <79601794011@ya.ru>"
    ],
    "workspaces": [
        "libs/*"
    ],
    "scripts": {
        "prepare": "husky",
        "start": "nx serve demo",
        "stylelint": "stylelint '**/*.{less,css}' --config package.json",
        "prettier": "prettier !package-lock.json . --ignore-path .gitignore",
        "lint": "eslint .",
        "release": "npx syncer && npx nx run-many -t publish -p angular,core,react"
    },
    "commitlint": {
        "extends": [
            "@taiga-ui/commitlint-config"
        ]
    },
    "lint-staged": {
        "*.{less,css,scss}": [
            "stylelint --fix"
        ],
        "*.{js,jsx,ts,txs,html,md,less,json}": [
            "prettier --write",
            "git add"
        ],
        "*.{js,ts}": [
            "eslint --fix",
            "git add"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "eslintConfig": {
        "extends": [
            "plugin:@taiga-ui/experience/all"
        ],
        "rules": {
            "no-console": "off",
            "no-restricted-imports": "off",
            "no-restricted-syntax": "off",
            "no-throw-literal": "off",
            "@angular-eslint/prefer-on-push-component-change-detection": "off",
            "@angular-eslint/prefer-output-readonly": "off",
            "@angular-eslint/prefer-standalone": "off",
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/explicit-member-accessibility": "off",
            "@typescript-eslint/member-ordering": "off",
            "@typescript-eslint/naming-convention": "off",
            "@typescript-eslint/no-unused-vars": "off",
            "@typescript-eslint/only-throw-error": "off",
            "@typescript-eslint/prefer-optional-chain": "off",
            "@typescript-eslint/promise-function-async": "off",
            "@typescript-eslint/require-await": "off",
            "@typescript-eslint/use-unknown-in-catch-callback-variable": "off",
            "import/no-import-module-exports": "off",
            "react/react-in-jsx-scope": "off",
            "rxjs/no-implicit-any-catch": "off",
            "rxjs/no-sharereplay": "off",
            "rxjs/no-topromise": "off",
            "rxjs/throw-error": "off",
            "sonarjs/no-base-to-string": "off",
            "sonarjs/no-ignored-exceptions": "off",
            "sonarjs/no-redundant-type-constituents": "off",
            "sonarjs/no-throw-literal": "off",
            "sonarjs/sonar-prefer-optional-chain": "off",
            "unicorn/filename-case": "off",
            "@angular-eslint/template/button-has-type": "off"
        },
        "root": true
    },
    "stylelint": {
        "extends": [
            "@taiga-ui/stylelint-config"
        ]
    },
    "dependencies": {
        "@angular/common": "17.3.12",
        "@angular/core": "17.3.12",
        "@angular/router": "17.3.12"
    },
    "devDependencies": {
        "@angular-architects/module-federation": "17.0.8",
        "@angular-devkit/build-angular": "17.3.10",
        "@angular-devkit/core": "17.3.10",
        "@angular-devkit/schematics": "17.3.10",
        "@angular/animations": "17.3.12",
        "@angular/cdk": "17.3.10",
        "@angular/cli": "17.3.10",
        "@angular/common": "17.3.12",
        "@angular/compiler": "17.3.12",
        "@angular/compiler-cli": "17.3.12",
        "@angular/core": "17.3.12",
        "@angular/forms": "17.3.12",
        "@angular/platform-browser": "17.3.12",
        "@angular/platform-browser-dynamic": "17.3.12",
        "@angular/router": "17.3.12",
        "@babel/core": "7.25.7",
        "@babel/preset-env": "7.25.7",
        "@babel/preset-react": "7.25.7",
        "@babel/preset-typescript": "7.25.7",
        "@ng-web-apis/common": "3.2.3",
        "@ngneat/spectator": "17.1.0",
        "@nx/angular": "19.3.0",
        "@nx/cypress": "19.3.0",
        "@nx/jest": "19.3.0",
        "@nx/js": "19.3.0",
        "@nx/node": "19.3.0",
        "@nx/react": "19.3.0",
        "@nx/rollup": "19.3.0",
        "@nx/web": "19.3.0",
        "@nx/webpack": "19.3.0",
        "@nx/workspace": "19.3.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
        "@schematics/angular": "17.3.10",
        "@svgr/webpack": "8.1.0",
        "@swc-node/register": "1.10.9",
        "@swc/cli": "0.4.0",
        "@swc/core": "1.7.28",
        "@swc/helpers": "0.5.13",
        "@taiga-ui/addon-doc": "3.95.1",
        "@taiga-ui/addon-mobile": "3.95.1",
        "@taiga-ui/auto-changelog-config": "0.219.0",
        "@taiga-ui/browserslist-config": "0.219.0",
        "@taiga-ui/cdk": "3.95.1",
        "@taiga-ui/core": "3.95.1",
        "@taiga-ui/eslint-plugin-experience": "0.219.0",
        "@taiga-ui/icons": "3.95.1",
        "@taiga-ui/kit": "3.95.1",
        "@taiga-ui/prettier-config": "0.219.0",
        "@taiga-ui/release-it-config": "0.219.0",
        "@taiga-ui/stylelint-config": "0.219.0",
        "@taiga-ui/syncer": "0.219.0",
        "@testing-library/react": "16.0.1",
        "@types/jest": "29.5.14",
        "@types/node": "20.17.17",
        "@types/react": "18.3.18",
        "@types/react-dom": "18.3.5",
        "@types/react-router-dom": "5.3.3",
        "@types/webpack": "5.28.5",
        "babel-core": "7.0.0-bridge.0",
        "babel-jest": "29.7.0",
        "core-js": "3.38.1",
        "css-loader": "7.1.2",
        "cypress": "13.15.0",
        "dotenv": "16.4.7",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "14.2.4",
        "lint-staged": "15.2.10",
        "ng-packagr": "17.3.0",
        "ngx-highlightjs": "10.0.0",
        "nx": "19.3.0",
        "postcss-preset-env": "9.6.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-refresh": "0.14.2",
        "react-router-dom": "6.26.2",
        "rxjs": "7.8.1",
        "style-loader": "4.0.0",
        "stylus": "0.63.0",
        "stylus-loader": "8.1.1",
        "ts-jest": "29.2.5",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "tslib": "2.7.0",
        "typescript": "5.2.2",
        "url-loader": "4.1.1",
        "webpack": "5.98.0"
    },
    "authors": [
        "Igor Katsuba <katsuba.igor@gmail.ru>",
        "Roman Sedov <79601794011@ya.ru>"
    ],
    "syncer": {
        "includePaths": [
            "./libs",
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "@microzord/*",
            "microzord"
        ],
        "ignorePackageNames": []
    }
}
